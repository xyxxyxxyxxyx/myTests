/* 水平间距 */
.cate {
  display: flex;
}
.cate .navigation_style {
  width: 120px;
  line-height: 60px;
  text-align: center;
}
.cate .navigation_style.active {
  background-color: #fff;
  position: relative;
}
.cate .navigation_style.active::before {
  content: '';
  position: absolute;
  background-color: #c00000;
  width: 3px;
  height: 30px;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  left: 3px;
}
.cate .right_list {
  line-height: 60px;
  background-color: #fff;
  text-align: center;
}
.cate .three_level {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  background-color: #fff;
}
.cate .three_level .it {
  width: 70px;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  background-color: #fff;
  margin-top: 10px;
}
.list {
  text-align: center;
}

